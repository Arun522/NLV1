{
  "name": "affinity-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src/ --ext .js,.jsx",
    "lint:all": "eslint . --ignore-path .eslintignore",
    "preview": "vite preview",
    "serve": "vite preview --port 3001",
    "build:prod": "NODE_ENV=production vite build",
    "build:staging": "NODE_ENV=staging vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "start": "npm run serve",
    "start:pm2": "pm2 start ecosystem.config.js --env production",
    "stop:pm2": "pm2 stop nextmaze-landing",
    "reload:pm2": "pm2 reload nextmaze-landing",
    "restart:pm2": "pm2 restart nextmaze-landing",
    "logs:pm2": "pm2 logs nextmaze-landing",
    "status:pm2": "pm2 status",
    "delete:pm2": "pm2 delete nextmaze-landing",
    "deploy:setup": "pm2 deploy ecosystem.config.js production setup",
    "deploy:prod": "pm2 deploy ecosystem.config.js production",
    "deploy:staging": "pm2 deploy ecosystem.config.js staging",
    "health-check": "curl -f http://localhost:3001 || exit 1",
    "test": "echo \"No tests specified\" && exit 0",
    "test:e2e": "echo \"E2E tests not implemented yet\" && exit 0",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules package-lock.json && npm install",
    "audit:fix": "npm audit fix",
    "audit:check": "npm audit --audit-level high",
    "precommit": "npm run lint && npm run build",
    "postinstall": "echo \"NextMaze Landing Page dependencies installed successfully\""
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "express": "^4.18.2",
    "framer-motion": "^12.23.12",
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "vite": "^7.1.0"
  }
}
